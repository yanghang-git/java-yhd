<?xml version="1.0"?>
<configuration>

	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
		<file>F:\yhdlog\backend.log</file>
		<encoder>
			<pattern>%date{yyyy-MM-dd HH:mm:ss} [%thread] - %msg %n</pattern>
		</encoder>
	</appender>

	<!-- 日志级别 -->
	<root>
		<level value="info" />
		<appender-ref ref="FILE" />
	</root>
</configuration>